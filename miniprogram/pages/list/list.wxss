page {
  background-color: #fefefe;
}
.Tab {
  position: fixed;
  height: 100%;
  width: 20%;
  background-color: #f7f7f7;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 33rpx;
  color: #888888;
}
.Tab .tab-item {
  padding: 20rpx;
  display: flex;
  flex-direction: row-reverse;
  justify-content: center;
  width: 100%;
}
.bowl {
  z-index: 6;
  position: fixed;
  left: 15rpx;
  bottom: 200rpx;
  width: 125rpx;
  height: 125rpx;
  background-color: #fff;
  box-shadow: 0 0 10rpx #888888;
  border-radius: 80rpx;
  display: flex;
  justify-items: center;
  align-items: center;
}
.bowl .bowlPic {
  position: relative;
  left: 12rpx;
  top: 5rpx;
  width: 100rpx;
  height: 100rpx;
}
.bowlPage-unfold {
  z-index: 10;
  position: fixed;
  height: 1000rpx;
  width: 560rpx;
  left: 23%;
  bottom: 300rpx;
  opacity: 0.9;
  background-color: #fff;
  border-radius: 30rpx;
  transition: all 0.5s ease-in-out;
}
.bowlPage-unfold .bowlScroll {
  height: 1000rpx;
}
.bowlPage-fold {
  z-index: 5;
  position: fixed;
  opacity: 0;
  background-color: #fff;
  bottom: 220rpx;
  height: 10rpx;
  width: 10rpx;
  left: 15%;
  transition: all 0.5s ease-in-out;
}
.content {
  position: relative;
  left: 25%;
  z-index: 5;
  font-size: 50rpx;
}
.content .intro {
  display: flex;
  flex-direction: column;
  margin-top: 37.5rpx;
  border-radius: 20rpx;
  align-items: center;
  width: 525rpx;
}
.content .intro .introImgBack {
  margin-top: 37.5rpx;
  height: 150rpx;
  width: 150rpx;
  border-radius: 25rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content .intro .introImgBack .introImg {
  position: relative;
  height: 100rpx;
  width: 100rpx;
}
.content .intro .introContent {
  margin: 20rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
}
.content .intro .introContent .content1 {
  font-weight: bolder;
  font-family: 'jianti';
}
.content .intro .introContent .content2 {
  margin-top: 20rpx;
  font-size: 30rpx;
}
.content .intro .introContent .content3 {
  margin-top: 20rpx;
  margin-bottom: 30rpx;
  font-size: 30rpx;
  font-weight: bolder;
}
.content .item {
  margin-top: 30rpx;
  box-shadow: 0 0 10rpx #888888;
  border-radius: 25rpx;
  width: 525rpx;
  height: 200rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: row;
  margin-bottom: 20rpx;
}
.content .item .itemPic {
  width: 100rpx;
  height: 100rpx;
  margin: 30rpx;
}
.content .item .itemPic .pic {
  width: 100rpx;
  height: 100rpx;
}
.content .item .itemContent {
  position: relative;
  right: 20rpx;
  display: flex;
  right: 5rpx;
  justify-content: space-between;
  flex-direction: column;
}
.content .item .itemContent .name {
  position: relative;
  bottom: 20rpx;
  font-weight: bolder;
  font-size: 45rpx;
}
.content .item .itemContent .cal {
  position: relative;
  top: 10rpx;
  margin-top: 10rpx;
  font-size: 30rpx;
}
.content .item .PAM {
  position: relative;
  top: -20rpx;
  display: flex;
  flex-direction: row;
  align-self: flex-end;
}
.content .item .PAM .num {
  font-size: 35rpx;
  width: 35rpx;
  position: relative;
  left: 8rpx;
  top: 5rpx;
}
.content .item .PAM .minus {
  width: 50rpx;
  height: 50rpx;
}
.content .item .PAM .minus .minusPic {
  width: 50rpx;
  height: 50rpx;
}
.content .item .PAM .plus {
  width: 50rpx;
  height: 50rpx;
}
.content .item .PAM .plus .plusPic {
  width: 50rpx;
  height: 50rpx;
}
