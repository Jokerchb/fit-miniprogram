.outter{
  /* height: 500rpx; */
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  transform: translateY( -500rpx); 
  -ms-transform: translateY( -500rpx); 
  -webkit-transform: translateY( -500rpx); 
}
.inner{
  /* height: 350rpx; */
  width: 90%;
  border-style: solid;
  box-shadow: 0rpx 0rpx 8rpx 8rpx #DDDDDD;
  border-width: 0rpx;
  border-radius: 20rpx;
}
.inner .title{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-weight: bold;
  font-size: 40rpx;
  color: #000000;
}
.inner .camera_img{
  width: 150rpx;
  height: 150rpx;
  margin-left: 262.5rpx;        /* 750*0.9/2-75 */
  margin-right: 262.5rpx;
  box-shadow: 0rpx 0rpx 8rpx 8rpx #DDDDDD;
  border-radius: 75rpx;

  background: url(data:image/png;base64,);
  background-size: 50% 50%;
  background-repeat: no-repeat;
  background-position: 50% 50%;

  /* 动画线性变化 等待1.5s 持续1s */
  /* animation: shake 1s linear;
  animation-delay: 1.5s; */
}
/* .inner .result{
  width: 100%;
  height: 300rpx;
} */
.inner .identify_img{
  width: 300rpx;
  margin-left: 187.5rpx;
  margin-right: 187.5rpx; 
  height: 300rpx;
  background: url("https://6669-fit-gc46z-1304760622.tcb.qcloud.la/prepage/%E8%AF%86%E5%88%AB%E7%BB%93%E6%9E%9C.png?sign=f051888590378f94853073298c3443d6&t=1617498497");
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.continue_btn{
  width: 100%;
  height: 200rpx;
  bottom: 0;
  position: fixed;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  transform: translateY( 200rpx); 
  -ms-transform: translateY( 200rpx); 
  -webkit-transform: translateY( 200rpx); 
}
.btn_icon{
  height: 64%;
  width: 128rpx;
  background: url("http://r.photo.store.qq.com/psc?/V10siXyS0lL9dw/TmEUgtj9EK6.7V8ajmQrELrZ36yEJLDuDTEHvc1hKMTwRQWNavkn*92hKF48HxGlSS0PBfR27kY81eI3bFcTb3i4.dZDpzN5oqR93klKoHo!/r");
  background-size: 100% 100%;
}

.btn_icon:after{
  content: "";
  background: #999;
  position: absolute; 
  width: 750rpx;
  height: 750rpx;
  left: calc(50% - 375rpx);
  top: calc(25% - 375rpx);
  opacity: 0;
  margin: auto;
  border-radius: 50%;
  transform: scale(1);
  transition: all 0.5s ease-in-out;
 }

.btn_icon:active:after {
  transform: scale(0);
  opacity: 1;
  transition: 0s;
}

/* 摄像头摇晃 */
@keyframes shake{
  0%  {
    rotate: 0deg;
  }
  25% {
    rotate:-45deg;
  }
  50% {
    rotate: 0deg;
  }
  62.5% {
    rotate: -30deg;
  }
  75% {
    rotate: 0deg;
  }
  81.25% {
    rotate: -15deg;
  }
  87.5% {
    rotate: 0deg;
  }
  93.75% {
    rotate: -7.5deg;
  }
  100% {
    rotate: 0deg;
  }
}