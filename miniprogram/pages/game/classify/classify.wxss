/* miniprogram/pages/game/classify/classify.wxss */
.whole{
  display: flex;
  flex-direction: row;
}

.back{
  width: 100%;
  background-color: rgb(255, 255, 255);
  display: flex;
  flex-direction: column;
}

.top{
  flex: 1;  
  display: flex;
  justify-content: center;
  align-items: center;
}

.heiban{
  width:450rpx;
  height:350rpx;
  background-repeat:no-repeat; /** 不重复*/
  background-size:contain; /**等比例缩放*/
  color: #fff;
  font-size: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-family: 'cola';
}

.table{
  flex: 2;
  align-self: flex-end;
  background-repeat:no-repeat; /** 不重复*/
  background-size:100% 100%; /**等比例缩放*/
  display: flex;
  flex-direction: column;
}

.transporter{
  flex: 4;
  display: flex;
  justify-content: center;
}

.food{
  width: 200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.Button{
  flex: 1;
  display: flex;
}

.leftButton{
  flex: 1;
  display: flex;
  justify-content: center;
}

.rightButton{
  flex: 1;
  display: flex;
  justify-content: center;
}
