
<view class="tab-bar" style="padding-bottom:{{bottom}}rpx" > 
  <!-- 为啥是零，chb不清楚， -->
  <view class="tab-bar-border"></view>
  <view class="tab-bar-item" data-path="/pages/homePage/homePage" data-index="0" bindtap="switchTab" >
    <image class="cover-image-slected" src="data:image/png;base64," wx:if="{{selected === 0}}"></image>
    <image class="cover-image" src="data:image/png;base64," wx:else></image>
  </view>
  <view class="tab-bar-item" data-path="/pages/list/list" data-index="1" bindtap="switchTab">
    <image class="cover-image-slected" src="data:image/png;base64," wx:if="{{selected === 1}}"></image>
    <image class="cover-image" src="data:image/png;base64," wx:else></image>
  </view>
  <view class="tab-bar-item" data-path="/pages/game/game" data-index="2" bindtap="switchTab">
    <image class="cover-image-slected" src="data:image/png;base64," wx:if="{{selected === 2}}"></image>
    <image class="cover-image" src="data:image/png;base64," wx:else></image>
  </view>
  <view class="tab-bar-item" data-path="/pages/mine/mine" data-index="3" bindtap="switchTab">
    <image class="cover-image-slected" src="data:image/png;base64," wx:if="{{selected === 3}}"></image>
    <image class="cover-image" src="data:image/png;base64," wx:else></image>
</view>
</view>
